{
	"cpu": "cortex-a8",
	"data-layout": "e-m:e-p:32:32-Fi8-i64:64-v128:64:128-a:0:32-n32-S64",
	"llvm-target": "arm-unknown-none-gnueabi",
	"target-endian": "little",
	"target-pointer-width": "32",
	"target-c-int-width": 32,
	"os": "tifflin",
	"arch": "arm",
		"linker-flavor": "ld",
		"linker": "arm-elf-eabi-ld",
		"pre-link-args": { "ld": ["-T", "link-armv7.ld", "-z","max-page-size=8192", "--gc-sections", "--start-group"] },
		"post-link-args": { "ld": ["--end-group"] },
		"features": "+v7,+db,-neon,-vfp2,-vfp3,-vfp4,-fp16,+soft-float",
		"executables": true,
		"dynamic-linking": true
}
